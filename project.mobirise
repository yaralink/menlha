          "_name": "features07",
          "_sourceTheme": "dinerm4",
          "_customHTML": "<section class=\"features07 dinerm4_features07\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Main Subtitle\" name=\"showSubTitle\">\n\t\t<input type=\"checkbox\" title=\"Main Title\" name=\"showmainTitle\">\n\t\t<input type=\"checkbox\" title=\"Tag\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showText\">\n\t\t<input type=\"checkbox\" title=\"Link\" name=\"showLink\" checked>\n\t\t<header>Color</header>\n\t\t<input type=\"color\" title=\"Card 1\" name=\"cardBg1\" value=\"#efefef\" selected>\n\t\t<input type=\"color\" title=\"Card 2\" name=\"cardBg2\" value=\"#efefef\" selected>\n\t\t<input type=\"color\" title=\"Card 3\" name=\"cardBg3\" value=\"#efefef\" selected>\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\" parallax>\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n\t\t</fieldset>\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<!-- End block parameters -->\n\t</mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n\n\t<div class=\"container\">\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-md-12 pb-5 col-lg-12 align-center\">\n\t\t\t\t<h2 class=\"mbr-section-subtitle mbr-exbold mbr-light mbr-fonts-style\" mbr-if=\"showSubTitle\" mbr-theme-style=\"display-5\">DEALS</h2>\n\t\t\t\t<h1 class=\"mbr-section-title pb-3 mbr-exbold mbr-fonts-style\" mbr-if=\"showmainTitle\" mbr-theme-style=\"display-2\">\n\t\t\t\t\tChoose your dishes\n\t\t\t\t</h1>\n\t\t\t</div>\n\n\t\t\t<div class=\"card item features-image col-12 col-md-10 col-lg-4\">\n\t\t\t\t<div class=\"item-wrapper card1\">\n\t\t\t\t\t<div class=\"card-box align-left mbr-white pt-4\">\n\t\t\t\t\t\t<h4 class=\"card-title pb-2 mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .card-img\">Appointment</h4>\n\t\t\t\t\t\t<p class=\"mbr-text mbr-bold pb-2 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .link\">\n\t\t\t\t\t\t\tVegan Set\n\t\t\t\t\t\t</p>\n\n\t\t\t\t\t\t<div class=\"link-align\" mbr-if=\"showLink\">\n\t\t\t\t\t\t\t<div class=\"link-inline\">\n\t\t\t\t\t\t\t\t<div class=\"link-wrap\">\n\t\t\t\t\t\t\t\t\t<h5 mbr-text class=\"link mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".link, .link-wrap, .link-align\">\n\t\t\t\t\t\t\t\t\t\t<a href=\"Appointment.html\" class=\"text-primary\"><b>Schedule NOW</b></a>\n\t\t\t\t\t\t\t\t\t</h5>\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbr-bold mobi-mbri-right mobi-mbri\"></span>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-img my-auto\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/1-260x260.png\" alt=\"Appointment\">\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<div class=\"card item features-image col-12 col-md-10 col-lg-4\">\n\t\t\t\t<div class=\"item-wrapper card2\">\n\t\t\t\t\t<div class=\"card-box align-left mbr-white pt-4\">\n\t\t\t\t\t\t<h4 class=\"card-title pb-2 mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .card-img\">\n\t\t\t\t\t\t\tDonation</h4>\n\t\t\t\t\t\t<p class=\"mbr-text mbr-bold pb-2 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .link\">\n\t\t\t\t\t\t\tMozzarella\n\t\t\t\t\t\t</p>\n\n\t\t\t\t\t\t<div class=\"link-align\" mbr-if=\"showLink\">\n\t\t\t\t\t\t\t<div class=\"link-inline\">\n\t\t\t\t\t\t\t\t<div class=\"link-wrap\">\n\t\t\t\t\t\t\t\t\t<h5 mbr-text class=\"link mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".link, .link-wrap, .link-align\">\n\t\t\t\t\t\t\t\t\t\t<a href=\"Donation.html\" class=\"text-primary\"><b>Donate NOW</b></a>\n\t\t\t\t\t\t\t\t\t</h5>\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbr-bold mobi-mbri-right mobi-mbri\"></span>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-img my-auto\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/2-260x260.png\" alt=\"Donation\">\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<div class=\"card item features-image col-12 col-md-10 col-lg-4\">\n\t\t\t\t<div class=\"item-wrapper card3\">\n\t\t\t\t\t<div class=\"card-box align-left mbr-white pt-4\">\n\t\t\t\t\t\t<h4 class=\"card-title pb-2 mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .card-img\">Body Check</h4>\n\t\t\t\t\t\t<p class=\"mbr-text mbr-bold pb-2 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .link\">\n\t\t\t\t\t\t\tBeef Steak\n\t\t\t\t\t\t</p>\n\n\t\t\t\t\t\t<div class=\"link-align\" mbr-if=\"showLink\">\n\t\t\t\t\t\t\t<div class=\"link-inline\">\n\t\t\t\t\t\t\t\t<div class=\"link-wrap\">\n\t\t\t\t\t\t\t\t\t<h5 mbr-text class=\"link mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".link, .link-wrap, .link-align\">\n\t\t\t\t\t\t\t\t\t\tCheck NOW\n\t\t\t\t\t\t\t\t\t</h5>\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbr-bold mobi-mbri-right mobi-mbri\"></span>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-img my-auto\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/3-260x260.png\" alt=\"Check your body\">\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</section>",
